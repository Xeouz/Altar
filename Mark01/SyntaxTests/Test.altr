func check_prime(n:int)
{
    if(n <= 1) 
    {
        return 0;
    }

    for(var i = 2; i < n; ++i)
    {
        if(n%i == 0)
        {
            return 0;
        }
    }

    return 1;
}

func main()
{
    var num = 7;
    if(check_prime(num) == 0)
    {
        echo("The number is not prime");
    }
    else 
    {
        echo("The number is prime");
    }
}